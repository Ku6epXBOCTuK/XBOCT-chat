{
	"name": "@ku6epxboctuk/sveltekit-template",
	"version": "0.0.1",
	"type": "module",
	"description": "Sveltekit template with prettier, eslint, vitest and playwright",
	"keywords": [
		"svelte",
		"sveltekit",
		"template",
		"web-dev"
	],
	"author": "Ku6epXBOCTuK",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Ku6epXBOCTuK/sveltekit-template.git"
	},
	"engines": {
		"node": ">=22.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:all": "npm run format && npm run lint:css-vars && npm run lint:css && npm run check  && npm run lint && npm run test:run",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"lint:css": "stylelint '**/*.{css,svelte}",
		"lint:css-vars": "node scripts/css-vars.js",
		"prepare": "svelte-kit sync || echo ''",
		"preview": "vite preview",
		"tauri": "tauri",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"test:run": "vitest run",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.2",
		"@eslint/js": "^9.39.2",
		"@playwright/test": "^1.58.1",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tauri-apps/cli": "^2.10.0",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.3.1",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^24",
		"@vitest/browser-playwright": "^4.0.18",
		"@vitest/coverage-istanbul": "^4.0.18",
		"@vitest/eslint-plugin": "^1.6.9",
		"@vitest/ui": "^4.0.18",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-playwright": "^2.7.1",
		"eslint-plugin-svelte": "^3.14.0",
		"glob": "^13.0.6",
		"globals": "^17.3.0",
		"jsdom": "^28.1.0",
		"playwright": "^1.58.1",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"stylelint": "^17.3.0",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-recommended": "^18.0.0",
		"svelte": "^5.51.0",
		"svelte-check": "^4.3.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0",
		"unplugin-icons": "^23.0.1",
		"vite": "^7.3.1",
		"vitest": "^4.0.18",
		"vitest-browser-svelte": "^2.0.2"
	}
}
